{
    "name": "othyn/filament-api-resources",
    "description": "A Laravel package that enables Filament to work seamlessly with API-backed resources instead of traditional Eloquent models",
    "keywords": [
        "laravel",
        "filament",
        "api",
        "resources",
        "rest",
        "livewire"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben",
            "email": "git@othyn.com",
            "homepage": "https://othyn.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/pagination": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "friendsofphp/php-cs-fixer": "^3.61.1"
    },
    "autoload": {
        "psr-4": {
            "Othyn\\FilamentApiResources\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Othyn\\FilamentApiResources\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Othyn\\FilamentApiResources\\FilamentApiResourcesServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "vendor/bin/php-cs-fixer fix"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
